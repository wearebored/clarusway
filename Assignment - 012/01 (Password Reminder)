depo = {
    "Enes": "şifreenes1",
    "Hakan": "şifrehakan2",
    "Hayati": "şifrehayati3",
    "Fatih": "şifrefatih4"
}
isim = input("İsminiz nedir?")
isim = isim.capitalize()
if isim in depo:
    print(f"Merhaba {isim}! Şifreniz: {depo[isim]}")
else:
    print(f"Merhaba, {isim}! Sonra görüşürüz")
